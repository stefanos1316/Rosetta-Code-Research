include "ConsoleWindow"

dim as short fizz, buzz, i
dim as Str15 s

for i = 1 to 100
  fizz = (i mod 3 )
  buzz = (i mod 5 )
   if (i)
      if fizz + buzz == 0 then print i; ".", "FizzBuzz" : exit if
      if fizz == 0 then print i; ".", "Fizz" : exit if
      if buzz == 0 then print i; ".", "Buzz" : exit if
     print i
   end if
next i
