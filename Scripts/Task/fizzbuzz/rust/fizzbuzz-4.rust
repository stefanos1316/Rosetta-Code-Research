// rust 0.8

fn main() {
    let mut n:uint = 1;
    while n <= 100 {
        if n % 15 == 0 {
            println("FizzBuzz");
        }
        else if n % 3 == 0 {
            println("Fizz");
        }
        else if n % 5 == 0 {
            println("Buzz");
        }
        else {
            println(n.to_str());
        }
        n += 1;
    }
}
