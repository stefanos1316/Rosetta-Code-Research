function testset(sequence s)
    for i=1 to length(s) do
        s[i] = isNumber(s[i])
    end for
    return s
end function
?testset({"#a","#A","0xA","0(16)A","#FF","255","0",".14",".05","-5.2","0xf","ten","1B"})
?testset({" 12  ",trim(" 12  ")})
?testset({"0o16","0o18"})
?testset({"1_000","0b10101111_11110000_11110000_00110011","-0b10101","0x10.5",""," ","1.","50e"})
