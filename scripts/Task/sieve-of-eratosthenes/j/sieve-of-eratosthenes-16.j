label=:dyad def 'echo x,":y'

sieve=:verb define
  'seq  ' label seq=: 2+i.y-1  NB. 2 thru y
  'n    ' label n=. 2
  'l    ' label l=. #seq
  whilst. -.seq-:prev do.
     prev=. seq
     'mask   ' label mask=. l{.1-(0{.~n-1),1}.l$n{.1
     'seq    ' label seq=. seq * mask
     'n      ' label n=. {.((n-1)}.seq)-.0
  end.
  seq -. 0
)

