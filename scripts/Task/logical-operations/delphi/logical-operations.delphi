program LogicalOperations;

{$APPTYPE CONSOLE}

const
  a = True;
  b = False;
begin
  Write('a = ');
  Writeln(a);
  Write('b = ');
  Writeln(b);
  Writeln;

  Write('a AND b: ');
  Writeln(a AND b);

  Write('a OR b: ');
  Writeln(a OR b);

  Write('NOT a: ');
  Writeln(NOT a);

  Write('a XOR b: ');
  Writeln(a XOR b);
end.
