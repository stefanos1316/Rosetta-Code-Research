constant s = {"Zig", "Zag", "Wally", "Ronald", "Bush", "Krusty", "Charlie", "Bush", "Boz", "Zag"}

integer r = find("Zag",s)       ?r  -- 2    (first)
r = find("Zag",s,r+1)           ?r  -- 10   (next)
r = find("Zag",s,r+1)           ?r  -- 0    (no more)
r = rfind("Zag",s)              ?r  -- 10   (last)
r = find("Zog",s)               ?r  -- 0    (none)
