#import system.
#import extensions.

#symbol program =
[
   #var a := console readLine toInt.
   #var b := console readLine toInt.

   (a < b)
       ? [ console writeLine:a:" is less than ":b. ].

   (a == b)
       ? [ console writeLine:a:" equals ":b. ].

   (a > b)
       ? [ console writeLine:a:" is greater than ":b. ].
].
