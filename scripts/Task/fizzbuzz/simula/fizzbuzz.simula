begin
    integer i;
    for i := 1 step 1 until 100 do
    begin
        if mod( i, 15 ) = 0 then
            outtext( "FizzBuzz" )
        else if mod( i, 3 ) = 0 then
            outtext( "Fizz" )
        else if mod( i, 5 ) = 0 then
            outtext( "Buzz" )
        else
            outint( i, 3 );
        outimage
    end;
end
