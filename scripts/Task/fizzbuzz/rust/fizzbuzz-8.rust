// rust 0.8

fn main() {
    for num in range(1,101) {
        let  answer =
         if  num % 15 == 0 {
             ~"FizzBuzz"
         }
         else if num % 3 == 0 {
             ~"Fizz"
         }
         else if num % 5 == 0 {
             ~"Buzz"
         }
         else {
            num.to_str()
         };
         println(answer);
    }
}
