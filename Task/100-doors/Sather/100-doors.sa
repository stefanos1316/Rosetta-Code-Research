class MAIN is
  main is
    pass, door :INT;
    doors :ARRAY{BOOL} := #(100);
    loop
      doors[0.upto!(99)] := false;
    end;
    pass := 0;
    loop while!(pass < 100);
      door := pass;
      loop while! (door < 100);
        doors[door] := ~doors[door];
	door := door + pass + 1
      end;
      pass := pass + 1;
    end;
    loop
      door := 0.upto!(99);
      #OUT + (door+1) + " " + doors[door] + "\n";
    end;
  end;
end;
